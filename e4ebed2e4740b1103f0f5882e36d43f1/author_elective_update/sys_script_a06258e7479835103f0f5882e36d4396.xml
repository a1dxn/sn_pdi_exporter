        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var duplicates = [];
    var gr = new GlideRecord(current.getTableName());
    gr.addQuery("u_repo_owner", current.u_repo_owner);
    gr.addQuery("u_repo_name", current.u_repo_name);
    gr.addQuery("u_branch_name", current.u_branch_name);
    gr.addQuery("sys_id", "!=", current.getUniqueValue());
    gr.query();
    while (gr.next()) {
        duplicates.push('<li><a href=/"' + gr.getLink(true) + '" target="_blank">' + gr.getValue("u_number") + "</a></li>");
    }

    if (duplicates.length) {
        var msg = "<h3>WARNING: There are multiple Export Configurations using the same repository branch. This will cause unexpected errors when pulling latest commits. Please create separate branches for each Export Configuration to avoid clashes.</h3><ul>" + duplicates.join() + "<ul>";
        gs.addErrorMessage(msg);
    }

})(current, previous);]]></script>
